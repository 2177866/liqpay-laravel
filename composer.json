{
    "name": "alyakin/liqpay-laravel",
    "description": "Laravel package for Liqpay integration",
    "keywords": [
        "alyakin",
        "Liqpay",
        "laravel",
        "payment"
    ],
    "homepage": "https://github.com/2177866/liqpay-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Lyakin",
            "email": "2177866@gmail.com",
            "role": "Developer"
        }
    ],
    "type": "library",
    "require": {
      "php": "^8.1",
      "illuminate/support": "^9.0|^10.0|^11.0",
      "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.35",
        "larastan/larastan": "*",
        "laravel/pint": "*"
    },
    "autoload": {
      "psr-4": {
        "Alyakin\\LiqpayLaravel\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
          "Alyakin\\LiqpayLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alyakin\\LiqpayLaravel\\LiqpayServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse",
        "format": "pint"
    }
  }
